{
  "name": "provider-twilio-receive-sms",
  "runtime": "nodejs",
  "customName": false,
  "customRole": false,
  "handler": "provider-twilio-receive-sms/handler.handler",
  "timeout": 10,
  "memorySize": 1024,
  "custom": {
    "excludePatterns": [],
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": ["Content-Type", "X-Amz-Date", "Authorization", "X-Api-Key"]
    }
  },
  "endpoints": [
    {
      "path": "providers/twilio/receive-sms",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {
        "integration.request.querystring.MessageSid": "method.request.querystring.MessageSid",
        "integration.request.querystring.SmsSid": "method.request.querystring.SmsSid",
        "integration.request.querystring.AccountSid": "method.request.querystring.AccountSid",
        "integration.request.querystring.MessagingServiceSid": "method.request.querystring.MessagingServiceSid",
        "integration.request.querystring.From": "method.request.querystring.From",
        "integration.request.querystring.To": "method.request.querystring.To",
        "integration.request.querystring.Body": "method.request.querystring.Body",
        "integration.request.querystring.NumMedia": "method.request.querystring.NumMedia"
      },
      "requestTemplates": {
        "application/json": "{\"MessageSid\":\"$input.params('MessageSid')\", \"SmsSid\":\"$input.params('SmsSid')\", \"AccountSid\":\"$input.params('AccountSid')\", \"MessagingServiceSid\":\"$input.params('MessagingServiceSid')\", \"From\":\"$input.params('From')\", \"To\":\"$input.params('To')\", \"Body\":\"$input.params('Body')\", \"NumMedia\":\"$input.params('NumMedia')\"}"
      },
      "responses": {
        "^400.*": {
          "statusCode": "400",
          "responseParameters": {
            "method.response.header.Access-Control-Allow-Origin": "'*'"
          }
        },
        "^401.*": {
          "statusCode": "401",
          "responseParameters": {
            "method.response.header.Access-Control-Allow-Origin": "'*'"
          }
        },
        "^404.*": {
          "statusCode": "404",
          "responseParameters": {
            "method.response.header.Access-Control-Allow-Origin": "'*'"
          }
        },
        "[^0-9].*": {
          "statusCode": "500",
          "responseParameters": {
            "method.response.header.Access-Control-Allow-Origin": "'*'"
          }
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {
            "method.response.header.Access-Control-Allow-Origin": "'*'",
            "method.response.header.Content-Type": "'text/xml'"
          },
          "responseModels": {},
          "responseTemplates": {
            "text/xml": "<?xml version=\"1.0\" encoding=\"UTF-8\" ?><Response><Message>$input.path('$.message')</Message></Response>"
          }
        }
      }
    }
  ],
  "events": [],
  "environment": "$${environment}"
}
