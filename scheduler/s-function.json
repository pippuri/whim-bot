{
  "name": "scheduler",
  "runtime": "nodejs4.3",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 30,
  "memorySize": 1024,
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [],
  "events": [
    {
      "name" : "MaaSScheduler",
      "type": "schedule",
      "config": {
        "schedule": "rate(1 minute)"
      }
    }
  ],
  "environment": "$${environment}"
}