{
  "restGet": {
    "application/json": {
      "method": "$context.httpMethod"
    }
  },
  "restPost": {
    "application/json": "{\n\"method\": \"$context.httpMethod\",\n\"body\": $input.json('$')\n}"
  },
  "restPut": {
    "application/json": "{\n\"method\": \"$context.httpMethod\",\"body\": $input.json('$'),\"id\":\"$input.params('id')\"\n}"
  },
  "restDelete": {
    "application/json": {
      "method": "$context.httpMethod",
      "id": "$input.params('id')"
    }
  },
  "environment": {
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_DATA_MODEL_STAGE": "${stage}",
    "SERVERLESS_PROJECT_NAME": "${project}",
    "AWS_REGION": "${AWS_REGION}",
    "IOT_ENDPOINT": "${IOT_ENDPOINT}",
    "SMS_CODE_SECRET": "${SMS_CODE_SECRET}",
    "JWT_SECRET": "${JWT_SECRET}",
    "TRIPGO_API_KEY": "${TRIPGO_API_KEY}",
    "HERE_APP_ID": "${HERE_APP_ID}",
    "HERE_APP_CODE": "${HERE_APP_CODE}",
    "TWILIO_ACCOUNT_SID": "${TWILIO_ACCOUNT_SID}",
    "TWILIO_ACCOUNT_TOKEN": "${TWILIO_ACCOUNT_TOKEN}",
    "TWILIO_FROM_NUMBER": "${TWILIO_FROM_NUMBER}",
    "API_BASE_URL": "${API_BASE_URL}",
    "WWW_BASE_URL": "${WWW_BASE_URL}",
    "HSL_PASSPHRASE": "${HSL_PASSPHRASE}",
    "HSL_USERTOKEN": "${HSL_USERTOKEN}",
    "MATKA_USERTOKEN": "${MATKA_USERTOKEN}",
    "MATKA_PASSPHRASE": "${MATKA_PASSPHRASE}",
    "COGNITO_POOL_ID": "${COGNITO_POOL_ID}",
    "COGNITO_DEVELOPER_PROVIDER": "${COGNITO_DEVELOPER_PROVIDER}",
    "COGNITO_PROFILE_DATASET": "${COGNITO_PROFILE_DATASET}",
    "COGNITO_USER_DEVICES_DATASET": "${COGNITO_USER_DEVICES_DATASET}",
    "GOOGLE_API_KEY": "${GOOGLE_API_KEY}",
    "DYNAMO_USER_PROFILE": "${DYNAMO_USER_PROFILE}",
    "DYNAMO_USER_TRAVEL_LOG": "${DYNAMO_USER_TRAVEL_LOG}",
