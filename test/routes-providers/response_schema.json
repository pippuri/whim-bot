{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://api.dev.maas.global/v1/provider/routes",
  "description": "Maas.fi routes provider response schema",
  "type": "object",
  "properties": {
    "plan": {
      "type": "object",
      "properties": {
        "itineraries": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "startTime": {"type": "number","minimum": 946684800000},
              "endTime": {"type": "number","minimum": 946684800000},
              "legs": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "mode": {"enum": ["WAIT", "WALK", "TRANSFER", "BUS", "TAXI", "RAIL"]}
                  },
                  "required": ["mode"]
                }
              }
            },
            "required": ["startTime", "endTime", "legs"]
          }
        }
      }
    }
  }
}
