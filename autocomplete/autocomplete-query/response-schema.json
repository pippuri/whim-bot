  "id": "https://api.maas.global/v1/autocomplete/response-schema.json",
  "description": "Response schema for autocomplete",
  "type": "object",
  "properties": {
    "suggestions": {
      "type": "array",
      "items": [
        {
          "type": "string"
        }
      ]
    },
    "maas": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "additionalProperties": false,
  "required": [ "suggestions" ]
}