{
  "name": "reverse-geocoding-query",
  "runtime": "nodejs4.3",
  "description": "MaaS reverse geocoding endpoint",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 10,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {},
  "endpoints": [
    {
      "path": "geocoding/reverse",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": true,
      "requestParameters": {
        "integration.request.querystring.locale": "method.request.querystring.locale",
        "integration.request.querystring.count": "method.request.querystring.count",
        "integration.request.querystring.lat": "method.request.querystring.lat",
        "integration.request.querystring.lon": "method.request.querystring.lon"
      },
      "requestTemplates": {
        "application/json": {
          "identityId": "",
          "payload": {
            "locale": "$input.params('locale')",
            "count": "$input.params('count')",
            "lat": "$input.params('lat')",
            "lon": "$input.params('lon')"
          }
        }
      },
      "responses": {
        "^400.*": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "200",
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": "$${environment}",
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
