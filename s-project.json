{
  "name": "MaaS",
  "custom": {
    "excludePatterns": [],
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": [
        "API-Revision",
        "Content-Type",
        "X-Amz-Date",
        "Authorization",
        "X-Api-Key"
      ]
    },
    "meta": {
      "name": "maas-serverless",
      "region": "eu-west-1"
    },
    "webpack": {
      "configPath": "webpack.config.js"
    },
    "client": {
      "bucketName": "staticfiles.maas.global"
    },
    "slack": {
      "token": "xoxb-55270714182-UY7cMnTgSTucKTnnzOvW4Gcl",
      "channel": "serverless",
      "endpoints": true,
      "functions": {
        "deployed": true,
        "removed": true
      }
    },
    "wrapper": {
      "path": "lib/wrapper/index.js"
    },
    "headerfunctions": [
      {
        "path": "lib/header-functions/register-subscription-filter.js",
        "hook": "function-post-deploy",
        "params": ["prod"]
      },
      {
        "path": "lib/header-functions/register-metric-alarm.js",		
        "hook": "function-post-deploy",		
        "params": [300, "arn:aws:sns:eu-west-1:756207178743:log-error-alarms"]		
      }
    ]
  },
  "plugins": [
    "serverless-cors-plugin",
    "serverless-wrapper-plugin",
    "serverless-webpack-plugin",
    "serverless-plugin-swagger-export",
    "serverless-plugin-autoprune",
    "serverless-meta-sync",
    "serverless-plugin-revision",
    "serverless-client-s3",
    "serverless-slack-plugin",
    "serverless-header-function"
  ],
  "version": "0.1.0",
  "profile": "serverless-v0.4.2",
  "location": "https://github.com/maasglobal/serverless-backend/",
  "author": "Sami Pippuri, Lauri Svan, Kenneth Falck, James Nguyen",
  "description": "MaaS serverless infrastructure",
  "swaggerExport": {
    "info": {
      "title": "MaaS Backend",
      "description": "Swagger documentation for maas-backend project, intended for app developer use",
      "termsOfService": "Private API, negotiate separately with MaaS Global",
      "contact": {
        "name": "MaaS Technical Team",
        "email": "tech@maas.global"
      },
      "version": "1.0"
    },
    "host": "api.dev.maas.global",
    "schemes": [
      "https"
    ]
  }
}
