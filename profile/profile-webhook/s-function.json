{
  "name": "profile-webhook",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: MaaS",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 20,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {},
  "endpoints": [
    {
      "path": "profile/webhook/{id}",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {
        "integration.request.path.id": "method.request.path.id"
      },
      "requestTemplates": {
        "application/json": {
          "id": "$input.params('id')",
          "payload": "$input.json('$')"
        }
      },
      "responses": {
        "^400.*": {
          "statusCode": "400"
        },
        "^403.*": {
          "statusCode": "403"
        },
        "^401.*": {
          "statusCode": "401"
        },
        "^404.*": {
          "statusCode": "404"
        },
        "^\\d{3}.*": {
          "statusCode": "500"
        },
        "default": {
          "statusCode": "200",
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": "$${environment}",
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
