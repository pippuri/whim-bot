{
  "name": "bookings-agency-options",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: MaaS",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 60,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {},
  "endpoints": [
    {
      "path": "bookings/options/{agencyId}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "CUSTOM",
      "authorizerFunction": "auth-custom-authorizer",
      "apiKeyRequired": true,
      "requestParameters": {
        "integration.request.path.agencyId": "method.request.path.agencyId",
        "integration.request.querystring.startTime": "method.request.querystring.startTime",
        "integration.request.querystring.endTime": "method.request.querystring.endTime",
        "integration.request.querystring.from": "method.request.querystring.from"
      },
      "requestTemplates": {
        "application/json": {
          "identityId": "$context.authorizer.principalId",
          "agencyId": "$input.params('agencyId')",
          "mode": "$input.params('mode')",
          "from": "$input.params('from')",
          "to": "$input.params('to')",
          "startTime": "$input.params('startTime')",
          "endTime": "$input.params('endTime')",
          "fromRadius": "$input.params('fromRadius')",
          "toRadius": "$input.params('toRadius')"
        }
      },
      "responses": {
        "^400.*": {
          "statusCode": "400"
        },
        "^403.*": {
          "statusCode": "403"
        },
        "^404.*": {
          "statusCode": "404"
        },
        "^\\d{3}.*": {
          "statusCode": "500"
        },
        "default": {
          "statusCode": "200",
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      },
      "swaggerExport": {
        "summary": "Retrieve agency available options",
        "description": "Retrieve all agency available options for transportation",
        "security": [
          {
            "Bearer": []
          }
        ],
        "parameters": [
          {
            "name": "agencyId",
            "in": "path",
            "description": "ID of the requested agency",
            "required": true,
            "type": "string"
          },
          {
            "name": "mode",
            "in": "query",
            "description": "Mode of transport",
            "required": true,
            "type": "string"
          },
          {
            "name": "from",
            "in": "query",
            "description": "Starting location.",
            "required": true,
            "type": "string"
          },
          {
            "name": "to",
            "in": "query",
            "description": "Destination location",
            "required": false,
            "type": "string"
          },
          {
            "name": "startTime",
            "in": "query",
            "description": "Starting time of the requested trip",
            "required": true,
            "type": "string"
          },
          {
            "name": "endTime",
            "in": "query",
            "description": "Ending time of the requested trip",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "application/json": {},
            "description": "Success",
            "schema": {
              "$ref": "maas-backend/bookings/bookings-agency-options/response.json"
            }
          },
          "400": {
            "description": "Invalid input"
          }
        }
      }
    }
  ],
  "events": [],
  "environment": "$${environment}",
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
