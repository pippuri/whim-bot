{
  "name": "bookings-agency-products",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: MaaS",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 20,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {},
  "endpoints": [
    {
      "path": "bookings/products/{agencyId}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "CUSTOM",
      "authorizerFunction": "auth-custom-authorizer",
      "apiKeyRequired": true,
      "requestParameters": {
        "integration.request.path.agencyId": "method.request.path.agencyId"
      },
      "requestTemplates": {
        "application/json": {
          "identityId": "$context.authorizer.principalId",
          "agencyId": "$input.params('agencyId')"
        }
      },
      "responses": {
        "^400.*": {
          "statusCode": "400"
        },
        "^403.*": {
          "statusCode": "403"
        },
        "^401.*": {
          "statusCode": "401"
        },
        "^404.*": {
          "statusCode": "404"
        },
        "^500.*": {
          "statusCode": "500"
        },
        "default": {
          "statusCode": "200",
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      },
      "swaggerExport": {
        "summary": "Retrieve agency available products",
        "description": "Retrieve all agency available products. This API is temporary, to be replaced 'real' product endpoints.",
        "security": [
          {
            "Bearer": []
          }
        ],
        "parameters": [
          {
            "name": "agencyId",
            "in": "path",
            "description": "ID of the requested agency",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "application/json": {},
            "description": "Success",
            "schema": {
              "$ref": "maas-backend/bookings/bookings-agency-products/response.json"
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    }
  ],
  "events": [],
  "environment": "$${environment}",
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
