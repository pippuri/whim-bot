{
  "name": "itinerary-update",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: MaaS",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 10,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "itineraries/{itineraryId}",
      "method": "PUT",
      "type": "AWS",
      "authorizationType": "CUSTOM",
      "authorizerFunction": "auth-custom-authorizer",
      "apiKeyRequired": false,
      "requestParameters": {
        "integration.request.path.itineraryId": "method.request.path.itineraryId"
      },
      "requestTemplates": {
        "application/json": "{\"itineraryId\": \"$input.params('itineraryId')\",\"payload\": $input.json()}"
      },
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {
            "application/json;charset=UTF-8": "Empty"
          },
          "responseTemplates": {
            "application/json;charset=UTF-8": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": "$${environment}",
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
