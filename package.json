  "scripts": {
    "test": "gulp test",
    "build:all": "npm run build:docs && npm run build:keys",
    "build:docs": "mkdir -p www/apidocs.maas.global/api && sls swagger export > www/apidocs.maas.global/api/maas-api.json && cp -r node_modules/maas-schemas/prebuilt/maas-backend www/apidocs.maas.global/api",
    "build:keys": "cd tickets/tickets-create/keys && ./decrypt-keys.sh -s alpha",
    "deploy:docs": "aws --profile maas s3 sync www/apidocs.maas.global/ s3://apidocs.maas.global/",
    "deploy-static-files": "aws --profile maas s3 sync --acl public-read static-files/dist s3://staticfiles.maas.global/",
    "deploy-dev:all": "npm run build:all && npm run test && sls function deploy -a -s dev && sls endpoint deploy -a -s dev",
    "deploy-prod:all": "npm run build:all && npm run test && sls function deploy -a -s prod && sls endpoint deploy -a -s prod",
    "deploy-test:all": "npm run build:all && npm run test && sls function deploy -a -s test && sls endpoint deploy -a -s test && npm run deploy:docs",
    "deploy-alpha:all": "npm run build:all && npm run test && sls function deploy -a -s alpha && sls endpoint deploy -a -s alpha",
    "release-alpha": "git checkout master && git pull --rebase upstream master && git checkout -B alpha && git rebase master && git push --force upstream alpha:alpha",
    "release-prod": "git checkout alpha && git pull --rebase upstream alpha && git checkout -B prod && git rebase prod && git push --force upstream prod:prod && npm run deploy-prod:all"
  }
