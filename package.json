  "scripts": {
    "test": "node ./scripts/check-env-vars.js && gulp test",
    "build:docs": "gulp build:swagger && sls swagger export > www/apidocs.maas.global/api/maas-api.json",
    "deploy:docs": "aws s3 cp --recursive --cache-control max-age=300 --acl public-read www/apidocs.maas.global/ s3://apidocs.maas.global/",
    "deploy:greenlist:dev": "aws s3 cp _meta/greenlists/greenlist-dev.json s3://maas-serverless/serverless/MaaS/greenlists/greenlist-dev.json",
    "deploy:greenlist:test": "aws s3 cp _meta/greenlists/greenlist-test.json s3://maas-serverless/serverless/MaaS/greenlists/greenlist-test.json",
    "deploy:greenlist:alpha": "aws s3 cp _meta/greenlists/greenlist-alpha.json s3://maas-serverless/serverless/MaaS/greenlists/greenlist-alpha.json",
    "deploy:greenlist:prod": "aws s3 cp _meta/greenlists/greenlist-prod.json s3://maas-serverless/serverless/MaaS/greenlists/greenlist-prod.json",
    "sync:greenlist:dev": "aws s3 cp s3://maas-serverless/serverless/MaaS/greenlists/greenlist-dev.json _meta/greenlists/greenlist-dev.json",
    "sync:greenlist:test": "aws s3 cp s3://maas-serverless/serverless/MaaS/greenlists/greenlist-test.json _meta/greenlists/greenlist-test.json",
    "sync:greenlist:alpha": "aws s3 cp  s3://maas-serverless/serverless/MaaS/greenlists/greenlist-alpha.json _meta/greenlists/greenlist-alpha.json",
    "sync:greenlist:prod": "aws s3 cp  s3://maas-serverless/serverless/MaaS/greenlists/greenlist-prod.json _meta/greenlists/greenlist-prod.json",
    "deploy:static-files": "aws s3 cp --recursive --acl public-read --cache-control max-age=300 _meta/static-files s3://staticfiles.maas.global/",
    "sync:static-files": "aws s3 cp --recursive s3://staticfiles.maas.global/ _meta/static-files/",
    "deploy-dev:all": "sls function deploy -a -s dev && sls event deploy -s dev && sls endpoint deploy -a -s dev",
    "deploy-test:all": "sls function deploy -a -s test && sls event deploy -s test && sls endpoint deploy -a -s test",
    "deploy-alpha:all": "sls function deploy -a -s alpha && sls event deploy -s alpha && sls endpoint deploy -a -s alpha",
    "deploy-prod:all": "sls function deploy -a -s prod && sls event deploy -s prod && sls endpoint deploy -a -s prod",
    "release-alpha": "git checkout master && git pull --rebase upstream master && git checkout -B alpha && git rebase master && git push --force upstream alpha:alpha",
    "release-prod": "git checkout alpha && git pull --rebase upstream alpha && git checkout -B prod && git rebase alpha && git push --force upstream prod:prod"
  }
