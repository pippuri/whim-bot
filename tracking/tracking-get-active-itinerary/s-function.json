{
  "name": "tracking-get-active-itinerary",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: MaaS",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 10,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {},
  "endpoints": [
    {
      "path": "tracking/active-itinerary",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "CUSTOM",
      "authorizerFunction": "auth-custom-authorizer",
      "apiKeyRequired": true,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": {
          "identityId": "$context.authorizer.principalId"
        }
      },
      "responses": {
        "^400.*": {
          "statusCode": "400"
        },
        "^401.*": {
          "statusCode": "401"
        },
        "^404.*": {
          "statusCode": "404"
        },
        "^\\d{3}.*": {
          "statusCode": "500"
        },
        "default": {
          "statusCode": "200",
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      },
      "swaggerExport": {
        "summary": "Get active itinerary",
        "description": "Gets the currently active itinerary. The response includes both the active itinerary and the active itinerary leg.",
        "security": [
          {
            "Bearer": []
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string",
                  "description": "Identifier of currently active itinerary"
                },
                "timestamp": {
                  "type": "number",
                  "description": "Time when the itinerary was activated (JavaScript timestamp)"
                },
                "activeLeg": {
                  "type": "object",
                  "description": "Currently active itinerary leg",
                  "properties": {
                    "id": {
                      "type": "string",
                      "description": "Identifier of currently active itinerary leg"
                    },
                    "timestamp": {
                      "type": "number",
                      "description": "Time when itinerary was activated (JavaScript timestamp)"
                    }
                  }
                },
                "startTime": {
                  "type": "number",
                  "description": "Route start time"
                },
                "endTime": {
                  "type": "number",
                  "description": "Route start time"
                },
                "legs": {
                  "type": "array",
                  "description": "List of itinerary legs in time order",
                  "items": {
                    "type": "object",
                    "properties": {}
                  }
                }
              }
            }
          }
        }
      }
    }
  ],
  "events": [],
  "environment": "$${environment}",
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
