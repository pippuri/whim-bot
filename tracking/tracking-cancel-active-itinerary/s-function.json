{
  "name": "tracking-cancel-active-itinerary",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: MaaS",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 10,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {},
  "endpoints": [
    {
      "path": "tracking/active-itinerary",
      "method": "DELETE",
      "type": "AWS",
      "authorizationType": "CUSTOM",
      "authorizerFunction": "auth-custom-authorizer",
      "apiKeyRequired": true,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": "{\"identityId\":\"$context.authorizer.principalId\"}"
      },
      "responses": {
        "^400.*": {
          "statusCode": "400"
        },
        "^401.*": {
          "statusCode": "401"
        },
        "^404.*": {
          "statusCode": "404"
        },
        "^\\d{3}.*": {
          "statusCode": "500"
        },
        "default": {
          "statusCode": "200",
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      },
      "swaggerExport": {
        "summary": "Cancel active route",
        "description": "Cancels any currently active route and route leg.",
        "security": [
          {
            "Bearer": []
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "properties": {}
            }
          }
        }
      }
    }
  ],
  "events": [],
  "environment": "$${environment}",
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
