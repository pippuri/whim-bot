{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://api.maas.global/v1/units",
  "description": "MaaS Common units",
  "definitions": {
    "arn": {
      "type": "string",
      "pattern": "^arn:aws:[a-z]+:[\\w\\-]*:\\d{12}:[ \\w\\-]+(\\/[\\w\\- \\/]+)*"
    },
    "cognitoPrincipalId": {
      "type": "string",
      "pattern": "^[aepus]{2}-[\\w]{4}-\\d:[a-f\\d]{8}(-[a-f\\d]{4}){3}-[a-f\\d]{12}$"
    }
  }
}
