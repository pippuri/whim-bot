{
  "name": "tickets-create",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: MaaS",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 10,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "tickets",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "NONE",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": {
          "partnerId": "$input.json('$.partnerId')",
          "partnerKey": "$input.json('$.partnerKey')",
          "startTime": "$input.json('$.startTime')",
          "endTime": "$input.json('$.endTime')",
          "from": "$input.json('$.from')",
          "to": "$input.json('$.to')",
          "ownerName": "$input.json('$.ownerName')",
          "custom": "$input.json('$.custom')",
          "domainId": "$input.json('$.domainId')",
          "meta": "$input.json('$.meta')"
        }
      },
      "responses": {
        "^400.*": {
          "statusCode": "400",
          "responseParameters": {
            "method.response.header.Access-Control-Allow-Origin": "'*'"
          }
        },
        "^401.*": {
          "statusCode": "401",
          "responseParameters": {
            "method.response.header.Access-Control-Allow-Origin": "'*'"
          }
        },
        "^404.*": {
          "statusCode": "404",
          "responseParameters": {
            "method.response.header.Access-Control-Allow-Origin": "'*'"
          }
        },
        "[^0-9].*": {
          "statusCode": "500",
          "responseParameters": {
            "method.response.header.Access-Control-Allow-Origin": "'*'"
          }
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {
            "method.response.header.Access-Control-Allow-Origin": "'*'"
          },
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": "$${environment}"
}
