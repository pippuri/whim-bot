{
  "name": "auth-custom-authorizer",
  "runtime": "nodejs4.3",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 10,
  "memorySize": 1024,
  "authorizer": {
    "type": "TOKEN",
    "identitySource": "method.request.header.Authorization"
  },
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
  ],
  "events": [],
  "environment": "$${environment}"
}
